[interface]
classification = virome

[input]
;; blast btab output 
$;INPUT_FILE_LIST$;=$;REPOSITORY_ROOT$;/output_repository/btabTrim/$;PIPELINEID$;_btab/btab.trim.list
$;INPUT_FILE$;=
$;INPUT_DIRECTORY$;=
;; the following is only used when iterating over an INPUT_DIRECTORY
$;INPUT_EXTENSION$;=raw

;; lookup mldbm dir location
$;LOOKUP_DIR$;=/diag/projects/virome/lookup

;; library list file eg. output of db-load-library
$;LIBRARY_LIST_FILE$;=$;REPOSITORY_ROOT$;/output_repository/db-load-library/$;PIPELINEID$;_default/db-load-library.txt.list

[output]
$;OUTPUT_TOKEN$;=mgol
$;OUTPUT_DIRECTORY$;=$;REPOSITORY_ROOT$;/output_repository/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;TXT_OUTPUT_LIST$;=$;OUTPUT_DIRECTORY$;/$;COMPONENT_NAME$;.txt.list

[component]
$;COMPONENT_NAME$;=blast-result-prep
$;DESCRIPTION$;=none
$;WORKFLOW_REPOSITORY$;=$;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;PIPELINE_TOKEN$;=unnamed

;The version,revision,tag here is set by an interpolated CVS tag
$;VERSION$;=1.0
$;RELEASE_TAG$;=$Name$
$;REVISION$;=$Revision: 1 $

$;TEMPLATE_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.xml
$;ITERATOR1$;=i1
$;ITERATOR1_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.$;ITERATOR1$;.xml

