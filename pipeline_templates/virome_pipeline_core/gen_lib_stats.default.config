[interface]
classification = virome

[parameter]
$;SERVER_NAME$; =
$;LIBRARY_ID$; = 
;;where is this executed igs, dbi, ageek or test
$;LOCATION$; = igs

[input]
;;use either one of the following inputs
;; Input expected is db-library output or library info file
$;INPUT_FILE_LIST$; = $;REPOSITORY_ROOT$;/output_repository/db-load-library/$;PIPELINEID$;_default/db-load-library.txt.list
$;INPUT_FILE$;=
$;INPUT_DIRECTORY$; = 
;; the following is only used when iterating over an INPUT_DIRECTORY
$;INPUT_EXTENSION$; = fsa

;;mldbm lookup file location dir
$;LOOKUP_DIR$;=/diag/projects/virome/lookup

[output]
$;OUTPUT_TOKEN$; = default
$;OUTPUT_DIRECTORY$; = /diag/projects/virome/virome-cache-files/$;PIPELINEID$;

[component]
$;COMPONENT_NAME$; = gen_lib_stats
$;DESCRIPTION$; = This script will process all libraries on a given server.  Get a break down of ORF types (missing 3', missing 5', incomplete, complete) ORF model (bacteria, archaea, phage) LIB type (viral only, microbial only, top viral, top microbial) FUNCTIONAL and UNClassified. Counts for each categories are stored in _cnt field, and all sequenceIds for each categories are stored in an external file.
$;WORKFLOW_REPOSITORY$; = $;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;PIPELINE_TOKEN$; = unnamed

;The version,revision,tag here is set by an interpolated CVS tag
$;VERSION$; = 1.0
$;RELEASE_TAG$; = $Name$
$;REVISION$;    = $Revision: 1 $

$;TEMPLATE_XML$; = $;DOCS_DIR$;/$;COMPONENT_NAME$;.xml
$;ITERATOR1$; = i1
$;ITERATOR1_XML$; = $;DOCS_DIR$;/$;COMPONENT_NAME$;.$;ITERATOR1$;.xml

;Distributed options
$;GROUP_COUNT$; = 150
$;NODISTRIB$; = 0

;the following keys are replaced at runtime by the invocation script
$;COMPONENT_CONFIG$; =
$;COMPONENT_XML$; =
$;PIPELINE_XML$; =
$;PIPELINEID$; =

[include]
$;PROJECT_CONFIG$;=

[dce]
;; These configuration options only apply to any distributed components
$;GROUP$;=
$;OS$;=
$;MEMORY$;=1
$;HOSTS$;=
$;QUEUE$;=workflow.q
$;DURATION$;=
$;PRIORITY$;=
$;PASSTHROUGH$;=
